<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SaveHospitalDetails</servlet-name>
        <servlet-class>SaveHospitalDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BloodBankFormData</servlet-name>
        <servlet-class>BloodBankFormData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BloodBankPostDisplayData</servlet-name>
        <servlet-class>BloodBankPostDisplayData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginBloodBank</servlet-name>
        <servlet-class>LoginBloodBank</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginHospital</servlet-name>
        <servlet-class>LoginHospital</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveHospitalDetails</servlet-name>
        <url-pattern>/SaveHospitalDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BloodBankFormData</servlet-name>
        <url-pattern>/BloodBankFormData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BloodBankPostDisplayData</servlet-name>
        <url-pattern>/BloodBankPostDisplayData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginBloodBank</servlet-name>
        <url-pattern>/LoginBloodBank</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginHospital</servlet-name>
        <url-pattern>/LoginHospital</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
